{
    "manifest_version": 3,
    "version": "1.3",
    "name": "Links tracker",
    "description": "This is an extension that helps you keep track of links you want to save",
    "icons": {
        "16": "icon.png",
        "32": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "permissions": [
        "tabs"
    ]
}